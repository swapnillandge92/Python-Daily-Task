   "source": [
    "# factorial of a number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the number:4\n",
      "Factorial of 4 is 24\n"
     ]
    }
   ],
   "source": [
    "def factorial(n):\n",
    " \n",
    " return 1 if (n==1 or n==0) else n * factorial(n - 1)\n",
    " \n",
    "num = int(input(\"Enter the number:\"))\n",
    "print (\"Factorial of\",num,\"is\",\n",
    "      factorial(num))\n"
   ]
  }
